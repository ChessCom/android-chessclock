<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CenteredButton">
        <attr name="buttonDrawable" format="dimension" />
        <attr name="buttonText" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="PanelInfoGameView">
        <attr name="oneLine" format="reference|boolean"/>
        <attr name="timeLeftHasBack" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="RoboTextView">
        <attr name="ttf" format="string" />
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <attr name="switchBackOn" format="reference" />
        <attr name="switchBackOff" format="reference" />
        <attr name="switchHandle" format="reference" />
        <attr name="switchHandleWidth" format="integer" />
        <attr name="switchHandleHeight" format="integer" />
        <attr name="switchHandleShift" format="float" />
        <attr name="switchHandleMargin" format="float" />
        <attr name="switchTextShift" format="float" />
        <attr name="switchTextSize" format="dimension|reference" />
        <attr name="switchTextColorOn" format="color|reference" />
        <attr name="switchTextColorOff" format="color|reference" />
        <attr name="switchTextFont" format="string" />
        <attr name="switchTextOn" format="string|reference" />
        <attr name="switchTextOff" format="string|reference" />
        <attr name="switchTextLeftPadding" format="integer" />
        <attr name="switchTextUseShadow" format="boolean" />
    </declare-styleable>



    <declare-styleable name="ButtonStyles">
        <attr name="greyButton" format="reference" />
        <attr name="orangeButton" format="reference" />
        <attr name="greyButtonSmall" format="reference" />
        <attr name="orangeButtonSmall" format="reference" />
        <attr name="greyButtonSmallSolid" format="reference" />
        <attr name="pageButton" format="reference" />
    </declare-styleable>


    <declare-styleable name="EnhancedField">
        <attr name="round_mode" format="reference|integer" />
        <attr name="color" format="reference|color" />
        <attr name="leftImage" format="reference|string" />
        <attr name="rightImage" format="reference|string" />
    </declare-styleable>

	<declare-styleable name="OrangeButtonTheme">
		<attr name="timerLabelStyle" format="reference"/>
	</declare-styleable>

    <declare-styleable name="com.inneractive.api.ads.InneractiveAd">
        <attr name="appID" format="string"/>

        <attr name="adType">
            <enum name="Banner" value="0"/>
            <enum name="Text" value="1"/>
            <enum name="Interstitial" value="2"/>
        </attr>

        <attr name="adAlignment">
            <enum name="TOP_LEFT" value="0"/>
            <enum name="TOP_CENTER" value="1"/>
            <enum name="TOP_RIGHT" value="2"/>
            <enum name="BOTTOM_LEFT" value="3"/>
            <enum name="BOTTOM_CENTER" value="4"/>
            <enum name="BOTTOM_RIGHT" value="5"/>
            <enum name="CENTER_LEFT" value="6"/>
            <enum name="CENTER" value="7"/>
            <enum name="CENTER_RIGHT" value="8"/>
        </attr>
        <attr name="refreshInterval" format="integer"/>
        <attr name="age" format="string"/>
        <attr name="gender" format="string"/>
        <attr name="keywords" format="string"/>
        <attr name="requiredAdWidth" format="integer"/>
        <attr name="requiredAdHeight" format="integer"/>
        <attr name="optionalAdWidth" format="integer"/>
        <attr name="optionalAdHeight" format="integer"/>
    </declare-styleable>


    <!--Sliding Menu Part-->
    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>

    <attr name="direction">
        <enum name="rightToLeft" value="0" />
        <enum name="bottomToTop" value="1" />
        <enum name="leftToRight" value="2" />
        <enum name="topToBottom" value="3" />
    </attr>

    <declare-styleable name="MultiDirectionSlidingDrawer">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="direction" />
        <attr name="bottomOffset" format="dimension"  />
        <attr name="topOffset" format="dimension"  />
        <attr name="allowSingleTap" format="boolean" />
        <attr name="animateOnClick" format="boolean" />
    </declare-styleable>
</resources>